<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <title>Hotelli</title>
    @Scripts.Render("~/bundles/modernizr")
    <!-- Favicons -->
    <link href="~/Content/Template/img/favicon.png" rel="icon">
    <link href="~/Content/Template/img/favicon.png" rel="icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Poppins:300,400,500,700" rel="stylesheet">

    <!-- Bootstrap CSS File -->
    <link href="~/Content/Template/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Libraries CSS Files -->
    <link href="~/Content/Template/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/Template/lib/animate/animate.min.css" rel="stylesheet">

    <!-- Main Stylesheet File -->
    <link href="~/Content/Template/css/style.css" rel="stylesheet">

    <!-- =======================================================
      Theme Name: Regna
