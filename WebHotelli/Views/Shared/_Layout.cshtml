    <script src="~/Content/Template/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/Template/lib/easing/easing.min.js"></script>
    <script src="~/Content/Template/lib/wow/wow.min.js"></script>

    <script src="~/Content/Template/lib/waypoints/waypoints.min.js"></script>
    <script src="~/Content/Template/lib/counterup/counterup.min.js"></script>
    <script src="~/Content/Template/lib/superfish/hoverIntent.js"></script>
    <script src="~/Content/Template/lib/superfish/superfish.min.js"></script>-->
    <!-- Contact Form JavaScript File -->
    <!--<script src="~/Content/Template/contactform/contactform.js"></script>-->
    <!-- Template Main Javascript File -->
    <!--<script src="~/Content/Template/js/main.js"></script>-->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

