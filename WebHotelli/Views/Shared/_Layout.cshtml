<body>

    <!--==========================
    Header
    ============================-->
    <header id="header" class="header-fixed">
        <div class="container">

            <div id="logo" class="pull-left">
                <a href="/"><img src="~/Content/Template/img/logo.png" alt="" title="" /></a>
            </div>

            <nav id="nav-menu-container">
                <ul class="nav-menu">
                    <li class="menu-active">@Html.ActionLink("Hotelli", "Index", "Home")</li>
                    <li>@Html.ActionLink("Reservacion", "Index", "Reservacion")</li>
                    <li>@Html.ActionLink("Habitacion", "Index", "Habitacion")</li>
                    <li>@Html.ActionLink("Categoria", "Index", "Categoria")</li>
                    <li>@Html.ActionLink("Usuario", "Index", "Usuario")</li>
                </ul>
            </nav><!-- #nav-menu-container -->
        </div>
    </header><!-- #header -->

    <div style="height: 72px"></div>

    @RenderBody()

    <!--==========================
      Footer
    ============================-->
    <footer id="footer">
        <div class="container">
            <div class="copyright">
                &copy; 2018 - Hotelli
            </div>
        </div>
    </footer><!-- #footer -->

    <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>

    <!-- JavaScript Libraries -->
    <!--<script src="~/Content/Template/lib/jquery/jquery.min.js"></script>
